{
  "manifest_version": 3,
  "name": "W2G Music Saver",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://w2g.tv/en/room/*"]
    }
  ],
  "action": {
    "default_popup": "pages/popup.html"
  }
}
